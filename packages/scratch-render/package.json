    "eslint": "^3.16.1",
    "eslint-config-scratch": "^3.1.0",
    "gh-pages": "^0.12.0",
    "hull.js": "0.2.10",
    "jsdoc": "^3.5.3",
    "json": "^9.0.4",
    "raw-loader": "^0.5.1",
