{
    "rules": {
        "curly": [2, "multi-line"],
        "eol-last": [2],
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "max-len": [2, 80, 4],
        "semi": [2, "always"],
        "strict": [2, "never"],
        "no-console": [2, {"allow": ["log", "warn", "error", "groupCollapsed", "groupEnd"]}],
        "valid-jsdoc": ["error", {"requireReturn": false}]
    },
    "env": {
        "node": true,
        "browser": true,
        "worker": true
    },
    "extends": "eslint:recommended"
}
